{
  "name": "react-mapfilter",
  "description": "A React Component for viewing and filtering GeoJSON",
  "version": "0.0.1",
  "main": "es5/index.js",
  "browserify": {
    "transform": [
      "babelify",
      "brfs"
    ]
  },
  "files": [
    "es5"
  ],
  "dependencies": {
    "base64-url": "^1.3.3",
    "case": "^1.4.2",
    "debug": "^2.2.0",
    "deep-equal": "^1.0.1",
    "drag-drop": "^2.11.0",
    "feature-filter": "^2.2.0",
    "feature-filter-geojson": "^1.0.0",
    "flat": "^2.0.1",
    "geojsonhint": "^1.2.0",
    "get-scrollbar-width": "^1.0.4",
    "insert-css": "^2.0.0",
    "is-plain-object": "^2.0.1",
    "lodash": "^4.16.4",
    "mapbox-gl": "^0.27.0",
    "material-ui": "^0.16.4",
    "moment": "^2.15.2",
    "object-assign": "^4.1.0",
    "randombytes": "^2.0.3",
    "react-addons-shallow-compare": "^15.3.2",
    "react-date-range": "^0.2.4",
    "react-imageloader": "^2.1.0",
    "react-intl": "^2.1.5",
    "react-intl-redux": "^0.1.0",
    "react-modal": "^1.5.2",
    "react-motion": "^0.4.5",
    "react-pure-render": "^1.0.2",
    "react-redux": "^4.4.0",
    "react-router": "^4.0.0-alpha.5",
    "react-router-redux": "^4.0.6",
    "react-tap-event-plugin": "^2.0.0",
    "react-virtualized": "^8.5.2",
    "recompose": "^0.20.2",
    "redbox-react": "^1.3.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4",
    "rework": "^1.0.1",
    "rework-import": "^2.1.0",
    "round-to": "^1.1.0",
    "turf-extent": "^1.0.4",
    "url-regex": "^3.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.5.2",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "brfs": "^1.4.3",
    "browserify": "^13.0.0",
    "browserify-hmr": "gmaclennan/browserify-hmr#gmaclennan-patch-1",
    "budo": "^8.0.4",
    "copyfiles": "^0.2.1",
    "dependency-check": "^2.5.1",
    "envify": "^3.4.1",
    "gh-pages": "^0.11.0",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.5.4",
    "spritesmith-cli": "^1.1.2",
    "spritezero-cli": "^1.0.2",
    "standard": "^8.5.0",
    "tape": "^4.4.0",
    "uglify-js": "^2.7.4",
    "uglifyify": "^3.0.4"
  },
  "license": "MIT",
  "scripts": {
    "dependency-check": "dependency-check ./package.json && dependency-check ./package.json --unused --no-dev",
    "lint": "standard",
    "start": "budo example/index.js:bundle.js --pushstate --dir example",
    "dev": "budo src/dev.js:bundle.js --pushstate --dir statics -- -p browserify-hmr",
    "test": "exit 0",
    "build": "mkdirp dist && npm run -s build:js && npm run -s build:min.js",
    "build:markers": "node ./bin/build_markers.js",
    "build:statics": "copyfiles -u 1 'statics/**/*' build",
    "build:js": "NODE_ENV=production browserify -g envify --standalone MapFilter src/index.js > dist/mapfilter.js",
    "build:es5": "rimraf es5 && babel src/ -d es5/",
    "build:min.js": "NODE_ENV=production browserify -g envify -g uglifyify --standalone MapFilter src/index.js | uglifyjs -c -m > dist/mapfilter.min.js",
    "clean": "rimraf dist es5",
    "prepublish": "npm run build:es5"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "build/**"
    ]
  },
  "peerDependencies": {
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  }
}
