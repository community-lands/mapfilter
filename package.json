{
  "name": "Monitoring-app",
  "description": "An app for viewing monitoring data",
  "version": "1.0.0-alpha1",
  "main": "src/index.js",
  "browserify": {
    "transform": [
      "babelify",
      "hbsfy",
      "browserify-css",
      "brfs"
    ]
  },
  "dependencies": {
    "base64-url": "^1.2.2",
    "debug": "^2.2.0",
    "deep-equal": "^1.0.1",
    "feature-filter": "^2.0.1",
    "flat": "^2.0.0",
    "geojsonhint": "^1.2.0",
    "lodash": "^4.12.0",
    "mapbox-gl": "^0.19.0",
    "material-ui": "^0.15.0",
    "moment": "^2.13.0",
    "randombytes": "^2.0.3",
    "react": "^15.0.2",
    "react-date-range": "^0.2.4",
    "react-dom": "^15.0.2",
    "react-pure-render": "^1.0.2",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.4",
    "react-tap-event-plugin": "^1.0.0",
    "recompose": "^0.18.0",
    "redbox-react": "^1.2.2",
    "redux": "^3.3.1",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.1",
    "round-to": "^1.1.0",
    "turf-extent": "^1.0.4",
    "url-regex": "^3.1.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-eslint": "^6.0.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.5.2",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "brfs": "^1.4.3",
    "browserify": "^13.0.0",
    "browserify-css": "^0.9.0",
    "browserify-hmr": "^0.3.1",
    "budo": "^8.0.4",
    "copyfiles": "^0.2.1",
    "dependency-check": "^2.5.1",
    "gh-pages": "^0.11.0",
    "handlebars": "^4.0.5",
    "hbsfy": "^2.5.0",
    "standard": "^7.0.1",
    "tape": "^4.4.0"
  },
  "license": "MIT",
  "scripts": {
    "dependency-check": "dependency-check ./package.json && dependency-check ./package.json --unused --no-dev",
    "lint": "standard",
    "start": "budo src/index.js:bundle.js",
    "dev": "budo src/dev.js:bundle.js -- -p browserify-hmr",
    "test": "babel-node test/*.test.js",
    "build": "npm run build:statics && npm run build:js",
    "build:statics": "copyfiles index.html fonts/* dist",
    "build:js": "NODE_ENV=production browserify src/index.js -o dist/bundle.js"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "js/**",
      "dist/**"
    ]
  }
}
