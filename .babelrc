{
  "presets": [
    "react",
    ["env", {
      "targets": {
        "browsers": ["last 2 versions", "not ie < 11"]
      },
      "shippedProposals": true
    }]
  ],
  "plugins": [
    [
      "react-intl", {
        "messagesDir": "./build/messages/"
      }
    ],
    "transform-class-properties",
    "./lib/babel-plugin-insert-css",
    "transform-runtime"
  ]
}
